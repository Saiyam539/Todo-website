{
    "vercel":2,
    "builds":[
        {
            "src":"testing/wsgi.py",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
        "src":"/static/(.*)",
        "dest":"/static/$1"
    },
    {
        "src":"/(.*)",
        "dest":"testing/wsgi.py"
    }
    ]
}